{
	"ecmaFeatures": {
		"globalReturn": false,
		"impliedStrict": true,
		"jsx": true
	},
	"ecmaVersion": 7,
	"env": {
		"browser": false,
		"es6": true,
		"node": true
	},
	"extends": [
		"eslint:recommended"
	],
	"root": true,
	"rules": {
		"no-console": [
			"off",
			{
				"allow": [
					"warn",
					"error"
				]
			}
		],
		"valid-jsdoc": [
			"error",
			{
				"requireReturn": false
			}
		]
	},
	"sourceType": "module"
}