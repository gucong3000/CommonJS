{
	"ecmaFeatures": {
		"globalReturn": false,
		"impliedStrict": true,
		"jsx": true
	},
	"ecmaVersion": 7,
	"env": {
		"browser": false,
		"es6": true,
		"node": true
	},
	"extends": [
		"eslint:recommended"
	],
	"root": true,
	"rules": {
		"indent": [
			"warn",
			"tab"
		],
		"no-console": [
			"off",
			{
				"allow": [
					"warn",
					"error",
					"info",
					"dir",
					"time",
					"debug"
				]
			}
		],
		"quotes": [
			"error",
			"double",
			{
				"allowTemplateLiterals": true,
				"avoidEscape": true
			}
		],
		"valid-jsdoc": [
			"error",
			{
				"requireReturn": false
			}
		]
	},
	"sourceType": "module"
}